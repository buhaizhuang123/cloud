<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.sys.dao.GroupMapper">


    <insert id="insertGroup" parameterType="com.cloud.sys.dto.GroupEntity">
        insert into S_GROUP
        (name, type,id,create_time,modify_time)
        value (#{name},#{type},#{id},#{createTime},#{modifyTime})
    </insert>
</mapper>